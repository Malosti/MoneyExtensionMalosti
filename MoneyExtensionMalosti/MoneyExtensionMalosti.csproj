<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <PackageId>MoneyExtensionMalosti</PackageId>
    <Version>1.0.0</Version>
    <License>MIT</License>
    <Authors>Guilherme Malosti Fonseca</Authors>
    <PackageDescription>Extension to convert decimal to integer</PackageDescription>
    <Description>Extension to convert decimal to integer</Description>
    <RepositoryUrl>https://github.com/Malosti/MoneyExtensionMalosti</RepositoryUrl>
